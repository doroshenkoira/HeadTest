<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:AppXamAndroid.ViewModels;assembly=AppXamAndroid"
              xmlns:ViewModels="clr-namespace:AppXamAndroid.ViewModels"
             x:Class="AppXamAndroid.Views.StudentsListPage" BackgroundImage="Sky.png">
 
  <ListView x:Name="StudentsLV">
    <ListView.ItemTemplate>
      <DataTemplate>
        <TextCell Text="{Binding studentName}" TextColor="#000000" />
      </DataTemplate>
    </ListView.ItemTemplate>
  </ListView>
</ContentPage>